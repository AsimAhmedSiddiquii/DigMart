<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <link rel="icon" type="image/png" href="/img/favicon.png">
    <title>Product Page</title>

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=2, user-scalable=no">
    <link href="/css/user/swiper-bundle.min.css" rel="stylesheet">
    <link href="/vendor/fontawesome/css/all.min.css" rel="stylesheet">
    <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/master.css" rel="stylesheet">
    <link href="/css/user/product.css" rel="stylesheet">

</head>

<body>
    <%- include('components/navbar') %>
        <!-- header part for product image and price  -->
        <div class="container mt-3 bg-white shadow rounded">
            <div class="row">

                <div class="col-lg-5 col-xs-12 mt-2">

                    <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
                        <div class="carousel-inner">
                            <% if(variantsData.length!=0){ %>
                                <div class="carousel-item active imgchgsize border border-dark">
                                    <img src="<%=variantData.images[0]%>" class=" img-fluid rounded-start mx-auto d-block" alt="...">
                                </div>
                                <% for (let a=1; a < variantData.images.length; a++){ %>
                                    <div class="carousel-item imgchgsize border border-dark">
                                        <img src="<%=variantData.images[a]%>" class="  img-fluid rounded-start mx-auto d-block" alt="...">
                                    </div>
                                    <%}%>
                                        <%}else{%>
                                            <div class="carousel-item active imgchgsize border border-dark">
                                                <img src="<%=productData.images[0]%>" class="img-fluid rounded-start mx-auto d-block" alt="...">
                                            </div>
                                            <% for (let a=1; a < productData.images.length; a++){ %>
                                                <div class="carousel-item imgchgsize border border-dark">
                                                    <img src="<%=productData.images[a]%>" class="img-fluid rounded-start mx-auto d-block" alt="...">
                                                </div>
                                                <% }} %>
                        </div>
                        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev"><h3><span class="carousel-control-prev-icon crousel-btn" aria-hidden="true">«</span></h3><span class="visually-hidden">Previous</span></button>
                        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next"><span class="carousel-control-next-icon crousel-btn" aria-hidden="true"><h3>»</h3></span><span class="visually-hidden">Next</span></button>
                    </div>

                    <div class="row">
                        <% if(variantsData.length!=0){ %>
                            <div class="col-md-12 p-0 d-flex">
                                <div id="prevProductBtn" class="fas fa-chevron-left fa-2x align-self-center"></div>
                                <div class="swiper flex-fill">
                                    <div class="swiper-wrapper mt-2">
                                        <% for (let b=0; b < variantData.images.length; b++){ %>
                                            <div class="ms-1 p-1 imgslidechgsize border border-dark">
                                                <img src="<%= variantData.images[b]%>" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="<%=b%>" class="img-fluid mx-auto d-block" aria-current="true" aria-label="Slide <%=b%>">
                                            </div>
                                            <%}%>
                                    </div>
                                </div>
                                <div id="nextProductBtn" class="fas fa-chevron-right fa-2x align-self-center"></div>
                            </div>
                            <%}else{%>
                                <div class="col-md-12 p-0">
                                    <!-- Slider main container -->
                                    <div class="swiper">
                                        <!-- Additional required wrapper -->
                                        <div class="swiper-wrapper mt-2">
                                            <% for (let b=0; b < productData.images.length; b++){ %>
                                                <div class="swiper-slide ms-1 p-1 imgslidechgsize border border-dark">
                                                    <img src="<%= productData.images[b]%>" width="100%" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="<%=b%>" class="img-fluid img-thumbnail border border-dark" aria-current="true" aria-label="Slide <%=b%>">
                                                </div>
                                                <%}%>
                                        </div>
                                        <!-- If we need navigation buttons -->
                                        <div id="prevProductBtn" class="fas fa-chevron-left fa-2x"></div>
                                        <div id="nextProductBtn" class="fas fa-chevron-right fa-2x"></div>
                                    </div>
                                </div>
                                <%}%>
                    </div>
                </div>

                <div class="col-lg-7 col-xs-12 mt-1">

                    <div>
                        <% if(variantsData.length!=0){ %>
                            <i class="fa fa-heart heart-icon mt-2" onclick="addwishlist(this, '<%=productData.sellerID%>','<%= productData._id%>', '<%=variantData._id%>')"></i>
                            <% } else { %>
                                <i class="fa fa-heart heart-icon mt-2" onclick="addwishlist(this, '<%=productData.sellerID%>','<%= productData._id%>',null)"></i>
                                <%}%>
                    </div>

                    <button class="share-btn"><i class="fas fa-share-square"></i></button>
                    <div class="share-options">
                        <p class="title">Share</p>
                        <div class="social-media">
                            <a role="button" onclick="shareonWhatsApp()" class="social-media-btn"><i class="fab fa-whatsapp"></i></a>
                        </div>
                        <div class="link-container">
                            <p class="link link-text"></p>
                            <button class="copy-btn" onclick="copyFunction()">copy</button>
                        </div>
                    </div>

                    <div class="card-body p-0 mt-2">
                        <h2 class="card-title text-dark">
                            <%= productData.productName %>
                        </h2>
                        <% if(rating != 0){ %>
                            <p class="card-text" style="font-size: 15px;"><span class="btn btn-sm  btn-success" style="font-size: 10px;"><%=rating%>&nbsp&nbsp<i class="fa fa-star"></i></span>&nbsp
                                <%= totalrating %> Ratings &
                                    <%= reviewData.length %> Reviews
                            </p>
                            <%}%>
                                <strong><p class="card-text fs-3 text-dark" id="noDiscount"></p></strong>
                                <div id="withDiscount">
                                    <p class="card-text fs-3 text-dark mb-0">
                                        <strong class="prodPrice" id="finalPrice"></strong><span class="ms-1 fs-6 text-black-20"><s id="actualPrice"></s></span>
                                        <span class="text-success fs-6" id="discount"></span>
                                    </p>
                                </div>
                                <% if(variantsData.length!=0){ %>
                                    <div>
                                        <strong>Color</strong>
                                        <div class="btn-group mt-1 pt-2 ms-2 mb-3" role="group" aria-label="Basic mixed styles example">
                                            <%var i=0; variantsData.forEach(function(data){ %>
                                                <button type="button" id="<%= i%>color" onclick="window.location.href='/product/variant/<%=data.slugID %>'" name="<%=data.colours %>" style="background-color: <%=data.colours %>;" class="color-btn btn border border-dark"></button>
                                                <% i++;}) %>
                                        </div>
                                    </div>

                                    <div>
                                        <strong>Size</strong>
                                        <div class="btn-group mt-1 pt-2 ms-2 mb-3" role="group" aria-label="Basic radio toggle button group">
                                            <% for (let i in variantData.sizes){ %>
                                                <button type="button" value="<%= i%>" class="btn btn-outline-primary" onclick="findsize('<%= variantData.sizes[i].sizes%>')" name="btnradio" id="<%= i%>size"><%=variantData.sizes[i].sizes %></button>
                                                <% } %>
                                        </div>
                                    </div>

                                    <div class="mt-3 mb-3">
                                        <a onclick="addcart('<%=productData.sellerID%>','<%= productData._id%>','<%=variantData._id%>','<%=variantData.colours%>')" id="variantCartBtn" class="header-color p-3"><i class="fa fa-cart-plus" style="color: white"></i><b> &nbsp;ADD TO CART</b></a>
                                        <a class="btn btn-outline-danger" id="outOfStock" style="display: none; ">Currently, Out of stock</a>
                                    </div>
                                    <%}else{%>
                                        <%if(productData.out_of_stock){%>
                                            <p class="text-danger ">Out of Stock</p>
                                            <%}else{%>
                                                <p class="text-success ">In Stock</p>
                                                <%}%>
                                                    <div class="mb-3 ">
                                                        <a onclick="addcart( '<%=productData.sellerID%>', '<%= productData._id%>',null,null) " class="header-color p-3 "><i class="fa fa-cart-plus " style="color: white "></i>&nbsp;<b>ADD TO CART</b></a>
                                                    </div>
                                                    <%}%>
                    </div>
                </div>
            </div>
        </div>

        <div class="container mt-3 rounded ">
            <div class="accordion shadow " id="accordionExample ">
                <div class="accordion-item ">
                    <h2 class="accordion-header mb-0 " id="headingOne ">
                        <button class="accordion-button " type="button " data-bs-toggle="collapse " data-bs-target="#collapseOne " aria-controls="collapseOne " style="color:white; background-color: #1e686dfa; ">Product Specifications</button>
                    </h2>
                    <div id="collapseOne " class="accordion-collapse collapse show " aria-labelledby="headingOne " data-bs-parent="#accordionExample ">
                        <div class="accordion-body ">
                            <strong>Description: </strong>
                            <%= productData.description %>
                                <% if(productData.specifications.length != 0) { %>
                                    <table class="table table-light table-striped mt-3 ">
                                        <tbody>
                                            <tr>
                                                <% if(productData.brand){ %>
                                                    <th scope="row ">Brand</th>
                                                    <td class="text-muted ">
                                                        <%=productData.brand%>
                                                    </td>
                                                    <%}%>
                                            </tr>
                                            <% for (let i in productData.specifications){ 
                                            if (productData.specifications[i].specName != " ") { %>
                                                <tr>
                                                    <td scope="row ">
                                                        <%=productData.specifications[i].specName %>
                                                    </td>
                                                    <td class="text-muted ">
                                                        <%=productData.specifications[i].specValue %>
                                                    </td>
                                                </tr>
                                                <% } else { %>
                                                    <tr>
                                                        <td>
                                                            No Product Specifications Found
                                                        </td>
                                                    </tr>
                                                    <% }} %>
                                        </tbody>
                                    </table>
                                    <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="container mt-3 rounded ">
            <div class="card shadow ">
                <div class="card-header header-color ">
                    Review and Ratings
                </div>
                <div class="card-body ">

                    <!-- Modal -->
                    <div class="modal fade " id="review-modal " data-bs-backdrop="static " data-bs-keyboard="false " tabindex="-1 " aria-labelledby="staticBackdropLabel " aria-hidden="true ">
                        <div class="modal-dialog ">
                            <div class="modal-content ">
                                <div class="modal-header ">
                                    <h5 class="modal-title " id="Label ">How Much did you Liked The Product</h5>
                                    <button type="button " class="btn-close " data-bs-dismiss="modal " aria-label="Close "></button>
                                </div>
                                <form action="/review " method="post " class="d-inline " enctype="multipart/form-data " id="ReviewForm ">
                                    <div class="modal-body ">
                                        <input type="hidden " id="sellerID " name="sellerID " value="<%=productData.sellerID%>">
                                        <input type="hidden" id="productID" name="productID" value="<%=productData._id%>">
                                        <div class="btn-group mb-3" role="group" aria-label="Basic radio toggle button group">
                                            <input type="radio" class="btn-check" name="btnradio" id="btnradio1" value="1" autocomplete="off" required>
                                            <label class="btn btn-lg btn-outline-danger" for="btnradio1">1</label>

                                            <input type="radio" class="btn-check" name="btnradio" id="btnradio2" value="2" autocomplete="off" required>
                                            <label class="btn btn-lg btn-outline-danger" for="btnradio2">2</label>

                                            <input type="radio" class="btn-check" name="btnradio" id="btnradio3" value="3" autocomplete="off" required>
                                            <label class="btn btn-lg btn-outline-warning" for="btnradio3">3</label>

                                            <input type="radio" class="btn-check" name="btnradio" id="btnradio4" value="4" autocomplete="off" required>
                                            <label class="btn btn-lg btn-outline-success" for="btnradio4">4</label>

                                            <input type="radio" class="btn-check" name="btnradio" id="btnradio5" value=5 autocomplete="off" required>
                                            <label class="btn btn-lg btn-outline-success" for="btnradio5">5</label>
                                        </div><br>
                                        <input type="file" class="form-control" name="images" id="images" multiple='multiple' accept="image/*">
                                        <br>
                                        <div class="mb-3">
                                            <label for="reviewHeadline" class="form-label text-dark">Add a Headline</label>
                                            <input type="text" class="form-control" id="reviewHeadline" name="reviewHeadline" required /><br>
                                            <label for="reviewText" class="form-label text-dark">Review & Feedback</label>
                                            <textarea class="form-control" id="reviewText" name="reviewText" rows="3" required></textarea>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <% if(rating !=0){ %>
                            <div class="col-md-8">
                                <p class="card-text text-dark"><span class="btn  btn-success" style="font-size: 15px"><i class="fa fa-star"> <%=rating%></i></span>&nbsp
                                    <%=totalrating%> Ratings &
                                        <%=reviewData.length%> Reviews
                                </p>
                            </div>
                            <% } else { %>
                                <div class="col-md-8">
                                    <p class="card-text text-black-30">
                                        NO RATING & REVIEWS YET
                                    </p>
                                </div>
                                <%}%>
                                    <div class="col-md-4">
                                        <button type="button" onclick="check('<%=productData._id%>')" class="btn header-color float-end" style="color: white">Rate Product</button>
                                    </div>
                    </div>
                    <hr>

                    <div class="container">
                        <div class="row g-3">
                            <%if(reviewData.length > 2){%>
                                <%for(let i=0; i < 2; i++){%>
                                    <div class="col-md-6">
                                        <div class="card">
                                            <div class="card-header">
                                                <% if(reviewData[i].rating > 3){ %>
                                                    <span class="btn btn-success"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                    <% } else if(reviewData[i].rating < 3){ %>
                                                        <span class="btn btn-danger"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                        <% } else { %>
                                                            <span class="btn btn-warning"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                            <%}%>
                                                                <b class="ms-1 mt-1"><%=reviewData[i].reviewHeadline%></b>
                                            </div>

                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="row">
                                                        <div class="col-md-12 p-0">
                                                            <p class="align-left mb-0 ms-2 fs-14">
                                                                <%= reviewData[i].reviewText %>
                                                            </p>
                                                        </div>
                                                    </div>
                                                    <%if(reviewData[i].images.length !=0){%>
                                                        <div class="row mt-2 mb-1">
                                                            <div class="col-md-12 p-0 ms-2" id="review-img">
                                                                <% for (let b=0; b < reviewData[i].images.length; b++){ %>
                                                                    <img src="<%= reviewData[i].images[b]%>" class="img-fluid img-thumbnail" onclick="imageModal('<%= reviewData[i].images[b]%>')">
                                                                    <%}%>
                                                            </div>
                                                        </div>
                                                        <%}%>

                                                </div>
                                            </div>
                                            <div class="card-footer text-muted fs-14">
                                                Review by
                                                <%=reviewData[i].userID.fullName%> on
                                                    <%=reviewData[i].date%>
                                            </div>
                                        </div>
                                    </div>
                                    <%}%>

                                        <div id="hidden-reviews" style="display: none;">

                                            <div class="row g-3">
                                                <%for(let i=2; i < reviewData.length; i++){%>
                                                    <div class="col-md-6">
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <% if(reviewData[i].rating > 3){ %>
                                                                    <span class="btn btn-success"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                                    <% } else if(reviewData[i].rating < 3){ %>
                                                                        <span class="btn btn-danger"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                                        <% } else { %>
                                                                            <span class="btn btn-warning"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                                            <%}%>
                                                                                <b class="ms-1 mt-1"><%=reviewData[i].reviewHeadline%></b>
                                                            </div>

                                                            <div class="card-body">
                                                                <div class="row">
                                                                    <div class="row">
                                                                        <div class="col-md-12 p-0">
                                                                            <p class="align-left mb-0 ms-2 fs-14">
                                                                                <%=reviewData[i].reviewText%>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                    <%if(reviewData[i].images.length !=0){%>
                                                                        <div class="row mt-2 mb-1">
                                                                            <div class="col-md-12 p-0 ms-2" id="review-img">
                                                                                <% for (let b=0; b < reviewData[i].images.length; b++){ %>
                                                                                    <img src="<%= reviewData[i].images[b]%>" class="img-fluid img-thumbnail" onclick="imageModal('<%= reviewData[i].images[b]%>')">
                                                                                    <%}%>
                                                                            </div>
                                                                        </div>
                                                                        <%}%>

                                                                </div>
                                                            </div>
                                                            <div class="card-footer text-muted fs-14">
                                                                Review by
                                                                <%=reviewData[i].userID.fullName%> on
                                                                    <%=reviewData[i].date%>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <%}%>
                                            </div>
                                        </div>

                                        <p id="view-reviews" onclick="show(this)"><i class="fas fa-arrow-down"></i> View all Reviews</p>
                                        <% } else { %>
                                            <% for(let i=0; i < reviewData.length; i++){ %>
                                                <div class="col-md-6">
                                                    <div class="card">
                                                        <div class="card-header">
                                                            <% if(reviewData[i].rating > 3){ %>
                                                                <span class="btn btn-success"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                                <% } else if(reviewData[i].rating < 3){ %>
                                                                    <span class="btn btn-danger"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                                    <% } else { %>
                                                                        <span class="btn btn-warning"><i class="fa fa-star"></i> <%=reviewData[i].rating%></span>
                                                                        <%}%>
                                                                            <b class="ms-1 mt-1"><%=reviewData[i].reviewHeadline%></b>
                                                        </div>

                                                        <div class="card-body">
                                                            <div class="row">
                                                                <div class="row">
                                                                    <div class="col-md-12 p-0">
                                                                        <p class="align-left mb-0 ms-2 fs-14">
                                                                            <%=reviewData[i].reviewText%>
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                                <%if(reviewData[i].images.length !=0){%>
                                                                    <div class="row mt-2 mb-1">
                                                                        <div class="col-md-12 p-0 ms-2" id="review-img">
                                                                            <% for (let b=0; b < reviewData[i].images.length; b++){ %>
                                                                                <img src="<%= reviewData[i].images[b]%>" class="img-fluid img-thumbnail" onclick="imageModal('<%= reviewData[i].images[b]%>')">
                                                                                <%}%>
                                                                        </div>
                                                                    </div>
                                                                    <%}%>

                                                            </div>
                                                        </div>
                                                        <div class="card-footer text-muted fs-14">
                                                            Review by
                                                            <%=reviewData[i].userID.fullName%> on
                                                                <%=reviewData[i].date%>
                                                        </div>
                                                    </div>
                                                </div>
                                                <% } %>
                                                    <% } %>
                        </div>
                    </div>

                </div>
            </div>
        </div>

        <div class="container mt-3 mb-3 rounded">
            <div class="card shadow">
                <div class="card-header header-color">
                    Related Products
                </div>
                <div class="card-body">
                    <div class="row">
                        <% 
                        if(similarProds.length!=0){
                            similarProds.forEach(function(product){ 
                        %>
                            <div class="col-lg-3 col-xs-6 col-md-6 col-sm-6 col-6">
                                <div class="card d-flex position-relative">
                                    <img class="card-img-top mt-1" style="height: 200px;" src="<%= product.images[0] %>">
                                    <div class="card-body">

                                        <h6 class="card-title">
                                            <%= product.prodID.productName %>
                                        </h6>

                                        <% if(product.sizes[0].discount){ %>
                                            <div class="position-absolute bg-success text-white discount-div px-2 fs-12 m-2">
                                                -
                                                <%= product.sizes[0].discount %>% off
                                            </div>
                                            <% } %>

                                                <p class="card-text mb-2">
                                                    <% if(product.sizes[0].discount){ %>

                                                        <span class="prodPrice">₹<%= product.sizes[0].finalPrice %></span>
                                                        <span class="text-muted fs-12"><s><%=product.sizes[0].actualPrice%></s></span>
                                                        <% } else { %>
                                                            <span class="prodPrice">₹<%= product.sizes[0].actualPrice %></span>
                                                            <% } %>
                                                </p>
                                                <a style="width: -webkit-fill-available; font-size: 14px;" class="btn btn-outline-primary" href="/product/variant/<%= product.slugID %>">View Product</a>
                                    </div>
                                </div>
                            </div>
                            <% })} %>
                    </div>
                </div>
            </div>
        </div>

        <%- include('components/footer') %>
            <!-- Login Modal -->
            <%- include('components/login') %>
                <!-- single image modal -->
                <%- include('../imageModal') %>
</body>

<script src="/vendor/jquery/jquery.min.js"></script>
<script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="/js/user/login.js"></script>
<script src="/js/user/swiper-bundle.min.js"></script>

<script>
    const finalPrice = document.getElementById("finalPrice");
    const actualPrice = document.getElementById("actualPrice");
    const discount = document.getElementById("discount");
    const noDiscount = document.getElementById("noDiscount");
    const withDiscount = document.getElementById("withDiscount");
    const outOfStock = document.getElementById("outOfStock");
    const variantCartBtn = document.getElementById("variantCartBtn");
    var count = 0;

    '<% if(variantsData.length!=0) { var i=0; %>'

    function findsize(size) {
        const btnval = window.event.target.value;
        $.ajax({
            url: "/product/findsize/<%=variantData._id%>/" + size,
            method: 'GET',
            headers: {
                'Content-Type': 'application/json'
            },
            success: function(res) {
                console.log(res)
                '<% if(variantsData.length!=0){ for (let i in variantData.sizes){ %>'
                document.getElementById('<%= i%>size').classList.remove('sizebutton')
                '<% } %>'
                count = btnval;
                document.getElementById(btnval + 'size').classList.add('sizebutton')
                if (res.discount == null) {
                    withDiscount.style.display = "none"
                    noDiscount.style.display = "inline"
                    noDiscount.innerText = '₹' + res.finalPrice
                } else {
                    withDiscount.style.display = "inline"
                    noDiscount.style.display = "none"
                    finalPrice.innerText = '₹' + res.finalPrice
                    actualPrice.innerText = "₹" + res.actualPrice
                    discount.innerHTML = res.discount + "% off"
                }
                //Out Of Stock
                if (res.out_of_stock) {
                    if (variantCartBtn) {
                        variantCartBtn.style.display = "none"
                    }
                    if (outOfStock) {
                        outOfStock.style.display = "inline"
                    }
                } else {
                    if (variantCartBtn) {
                        variantCartBtn.style.display = "inline"
                    }
                    if (outOfStock) {
                        outOfStock.style.display = "none"
                    }
                }
            }
        })
    }

    if ('<% variantData.sizes[0].discount%>' == null) {
        withDiscount.style.display = "none"
        noDiscount.style.display = "inline"
        noDiscount.innerText = '₹<%=variantData.sizes[0].finalPrice%>'
    } else {
        withDiscount.style.display = "inline"
        noDiscount.style.display = "none"
        finalPrice.innerText = '₹<%=variantData.sizes[0].finalPrice%>'
        actualPrice.innerText = "₹<%=variantData.sizes[0].actualPrice%>"
        discount.innerHTML = '<%=variantData.sizes[0].discount%>' + "% off"
    }


    if ('<%= variantData.sizes[0].out_of_stock %>' == 'true') {
        if (variantCartBtn) {
            variantCartBtn.style.display = "none"
        }
        if (outOfStock) {
            outOfStock.style.display = "inline"
        }
    }
    document.getElementById('0size').classList.add('sizebutton')

    '<%variantsData.forEach(function(data){ %>'
    ' <%if(data.colours==variantData.colours){%>'
    document.getElementById('<%= i%>color').classList.add('colorbutton')
    '<% } i++; })} %>'
    '<% } else { %>'
    if ('<%=productData.discount%>' == null) {
        withDiscount.style.display = "none"
        noDiscount.style.display = "inline"
        noDiscount.innerText = '₹ <%=productData.finalPrice%>'
    } else {
        withDiscount.style.display = "inline"
        noDiscount.style.display = "none"
        finalPrice.innerText = '₹ <%=productData.finalPrice%>'
        actualPrice.innerText = "₹ <%=productData.actualPrice%>"
        discount.innerHTML = '<%=productData.discount%>' + "% off"
    }
    '<% } %>'
</script>

